<h1>Listing news</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Url</th>
    <th>Description</th>
    <th></th>
    <th></th>
  </tr>

<% @news.each do |news| %>
  <tr>
    <td><%= news.title %></td>
    <td><%= news.url %></td>
    <td><%= news.description %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New News', new_news_path %>
