<h1>Listing rankings</h1>

<div class="row-fluid">
<div class="span8 offset2">
<table class="table table-striped">
<% @rankings.news.each do |news| %>
  <tr>
  	<td><%= news.rank %></td>
    <td><%= news.title %></td>
    <td><%= news.url %></td>
    <td><%= news.description %></td>
    <td><%= news.change_rank %></td>
  </tr>
<% end %>
</table>
</div>
</div>

<br />
